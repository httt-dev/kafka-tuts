      "topic.creation.default.cleanup.policy": "delete",    
      "topic.creation.default.retention.ms": "86400000" ,
  
      "table.exclude.list": ".*\\.TMP_.*,.*\\.SYS_TEMP_.*,.*\\$_.*",
      "log.mining.skip.unparseable.dml.statements": "true",
      "log.mining.dml.parser.skip.errors": "true",
      "log.mining.query.filter.mode": "regex",
      "log.mining.transaction.retention.hours": "0"
